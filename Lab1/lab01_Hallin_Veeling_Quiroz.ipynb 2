      "[M, lamb_exp] = np.unravel_index(np.matrix.argmin(np.asmatrix(errors)), errors.shape)\n",
      "print 'M = '+str(M)+', lamb = e^-' + str(lamb_exp)\n",
      "plt.plot(x,t,label='data')\n",
      "w, Phi = fit_polynomial_reg(x, t, M,np.exp(-lamb_exp))\n",
      "y = Phi.dot(w.T)\n",
      "plt.plot(x,y,label='M='+str(M))\n",
      "plt.legend()\n",
      "plt.show()"
