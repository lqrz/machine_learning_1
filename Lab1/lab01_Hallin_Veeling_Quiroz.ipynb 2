     "input": [
      "[x_train,t] = gen_sinusoidal2(7)\n",
      "M = 5\n",
      "alpha = 1./2.\n",
      "beta = 1./0.04\n",
      "pos_mean, pos_cov = fit_polynomial_bayes(x_train, t, M, alpha, beta)\n",
      "plot_N = 50\n",
      "x_plot = np.linspace(0,np.pi*2,plot_N)\n",
      "\n",
      "Theta = np.random.multivariate_normal(pos_mean,pos_cov,100)\n",
      "\n",
      "for theta in Theta:\n",
      "    Phi = design_matrix(x_plot,M)\n",
      "    y = Phi.dot(theta.T)\n",
      "    plt.plot(x_plot,y,color='red',alpha=0.1)\n",
      "plt.plot(x_train,t, 'o')\n",
      "x1,x2,y1,y2 = plt.axis()\n",
      "plt.axis((x1, x2, np.amin(t)*2, np.amax(t)*2))\n",
      "plt.show()"
